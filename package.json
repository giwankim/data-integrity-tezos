{
  "name": "tezos-example",
  "version": "2.0.0",
  "description": "Tezos Example Box =================",
  "dependencies": {
    "@taquito/taquito": "^9.1.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "ganache-cli": "^6.12.1-tezos.0"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "compile": "yarn clean && truffle compile",
    "migrate": "yarn clean && truffle migrate",
    "migrate-florencenet": "yarn clean && truffle migrate --network florencenet",
    "migrate-mainnet": "yarn clean && truffle migrate --network mainnet",
    "start-sandbox": "ganache-cli --flavor tezos --seed alice",
    "test": "yarn clean && yarn compile && truffle test",
    "test:quick": "truffle test"
  },
  "license": "MIT"
}